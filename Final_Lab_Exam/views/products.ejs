<h1>All Products</h1>
<ul style="display: flex;">
  <% products.forEach(product => { %>
    <div class="product">
                    <a href="/products/<%= product._id %>">
                    <img src="<%= product.image %>" alt="">
                    <p><%= product.name %></p>
                    <p><%= product.price %></p>
                    </a>
                </div>

  <% }) %>
</ul>
